{
  "name": "Genius Phone Repair",
  "version": "1.0.0",
  "description": "Genius Phone Repair - Website Upgrade by Innogenx",
  "main": "app.js",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "dev": "concurrently \"npm run build-dev\" \"nodemon\"",
    "dev2": "concurrently \"copy .env.dev .env\" \"npm run build-dev\" \"nodemon\"",
    "build-dev": "webpack --config webpack.config.dev.js --watch",
    "prod": "concurrently \"npm run build-prod\" \"node --config-file ./Server/.babelrc app.js\"",
    "prod2": "concurrently \"copy .env.prod .env\" \"npm run build-prod\" \"node --config-file ./Server/.babelrc app.js\"",
    "build-prod": "webpack --config webpack.config.prod.js"
  },
