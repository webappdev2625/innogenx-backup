    "csurf": "latest",
    "debug": "latest",
    "dotenv": "latest",
    "express": "latest",
    "express-session": "latest",
    "express-static-gzip": "^2.1.7",
    "helmet": "^5.0.2",
    "http-errors": "latest",
    "http2-express-autopush": "^1.0.5",
    "http2-express-bridge": "^1.0.7",
